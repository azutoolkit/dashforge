{% extends "layout.jinja" %}

{% block styles %}
<link href="/assets/lib/@fortawesome/fontawesome-free/css/all.min.css" rel="stylesheet">
<link href="/assets/lib/ionicons/css/ionicons.min.css" rel="stylesheet">
<link rel="stylesheet" href="/assets/css/dashforge.css">
<link rel="stylesheet" href="/assets/css/dashforge.dashboard.css">
{% endblock  %}

{% block page_body %}
<div class="row">
  <div class="col col-3">
    {{queue_overview}}
  </div>
  <div class="col col-9">
    <div class="row row-xs">
      <div class="col-lg">
        {% include 'queues/partials/total_jobs.jinja.html' %}
      </div>
      <div class="col-lg">
        {{ errors_chart }}
      </div>
      <div class="col-lg">
        {% include 'queues/partials/latency.jinja.html' %}
      </div>
    </div>
    <hr />
    <div class="row row-xs">
      <div class="col-lg">
        {% include 'queues/partials/queue_table.jinja.html' %}
      </div>
    </div>
  </div>
</div>
{% endblock %}

{% block script_footer %}
<script src="/assets/lib/jquery/jquery.min.js"></script>
<script src="/assets/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="/assets/lib/feather-icons/feather.min.js"></script>
